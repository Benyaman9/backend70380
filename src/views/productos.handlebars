<h1>Lista de productos</h1>
<ul>
    {{! metodo foreach en handlebarssss}}
    {{! consulto verdadero o falsi si existe o no }}
    {{#each productos}}
    {{!aqui consulto los atributos del producto}}
    {{#if status}}
    <h3>Producto {{nombre}}</h3>
    <p>Marca: {{marca}}</p>
    <p>Precio: ${{precio}}</p>
    <p>Stock: {{stock}} unidades</p>
    {{/if}}
    {{/each}}
</ul>